<div class="popdown pop-style"><h1></h1></div>
<% if @message %>
  <script>
  $(function() {
    $( "#dialog-message" ).dialog({
      modal: true,
      buttons: {
        Ok: function() {
          $( this ).dialog( "close" );
        }
      }
    });
  });
  </script>  

 
<div id="dialog-message" title="Link Not Found!!">
  <p>
    <span class="" style="float: left; margin: 0 7px 50px 0;"></span>
    <%= @message %>
  </p>
</div>
<% end %>
